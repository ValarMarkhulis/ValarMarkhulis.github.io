// Write a character to the USART1 Transmitter
#pragma used+
void putchar1(char c)
{
while ((UCSR1A & DATA_REGISTER_EMPTY)==0);
UDR1=c;
}

/*Funktionen modtager en pointer til et array og udskriver arrayet til terminalen med put_char funktionen*/
void putsUSART0(char *array) {
  
  while(*array != '\0') // '\r' er slut karakteren
    { putchar1(*array);
  array++;}
}

